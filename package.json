{
  "name": "timer",
  "version": "2.2.4",
  "description": "a timer for debate competition",
  "scripts": {
    "test": "node src/test.js",
    "build": "webpack --env.production",
    "deploy": "npm run build && gulp deploy",
    "dev": "webpack-dev-server --debug --devtool cheap-module-source-map --output-pathinfo --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccoode/timer.git"
  },
  "author": "dgeibi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ccoode/timer/issues"
  },
  "homepage": "https://github.com/ccoode/timer#readme",
  "devDependencies": {},
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 3 firefox versions",
              "chrome > 48",
              "last 2 edge versions",
              "last 2 safari versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.16.0",
    "babili-webpack-plugin": "^0.0.11",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "i": "^0.3.5",
    "json-loader": "^0.5.4",
    "performance-now": "^2.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-csso": "^1.1.2",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.2",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
